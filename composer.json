{
    "name": "brainsum/generic_dam",
    "type": "drupal-module",
    "description": "Feature-set for turning a Drupal 8 site into a Digital Assets Management platform.",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "mhavelant",
            "email": "mhavelant@brainsum.com"
        }
    ],
    "repositories": {
        "drupal-8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "drupal/core": "^8.7",
        "drupal/focal_point": "^1.2",
        "drupal/maillog": "1.x-dev"
    },
    "require-dev": {
        "drupal-composer/drupal-security-advisories": "8.x-dev",
        "roave/security-advisories": "dev-master",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}

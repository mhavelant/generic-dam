{
    "name": "brainsum/media_assets",
    "description": "Assets module for media assets.",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "mhavelant",
            "email": "mhavelant@brainsum.com"
        }
    ],
    "repositories": {
        "drupal-8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "media_upload": {
            "type": "vcs",
            "url": "https://github.com/brainsum/media_upload.git"
        },
        "dropzone": {
            "type": "package",
            "package": {
                "name": "enyo-drupal/dropzone",
                "version": "v4.3.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "drupal/core": "^8.7",
        "drupal/focal_point": "^1.2",
        "drupal/media_upload": "^1.7",
        "enyo-drupal/dropzone": "^4.3"
    }
}
